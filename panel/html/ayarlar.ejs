  <script>
      $(document).ready(function(){
        
        $("#genelAyarlarBtn").click(function() {
          $("#genelAyarlarBtn").addClass("activex");
          $("#genelAyarlar").css("display", "block");
          $("#ozelKomutlarBtn").removeClass("activex");
          $("#ozelKomutlar").css("display", "none");
          $("#modAyarlarBtn").removeClass("activex");
          $("#modAyarlar").css("display", "none");
        });
        
        $("#ozelKomutlarBtn").click(function() {
          $("#genelAyarlarBtn").removeClass("activex");
          $("#genelAyarlar").css("display", "none");
          $("#ozelKomutlarBtn").addClass("activex");
          $("#ozelKomutlar").css("display", "block");
          $("#modAyarlarBtn").removeClass("activex");
          $("#modAyarlar").css("display", "none");
        });
        
        $("#modAyarlarBtn").click(function() {
          $("#genelAyarlarBtn").removeClass("activex");
          $("#genelAyarlar").css("display", "none");
          $("#ozelKomutlarBtn").removeClass("activex");
          $("#ozelKomutlar").css("display", "none");
          $("#modAyarlarBtn").addClass("activex");
          $("#modAyarlar").css("display", "block");
        });
        
      });
  </script>
  
